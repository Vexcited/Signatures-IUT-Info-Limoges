{
  "name": "signatures-iut-limoges-monorepo",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "library:release": "pnpm --filter signatures-iut-limoges run release",
    "library:test": "pnpm --filter signatures-iut-limoges run test",
    "library:build": "pnpm --filter signatures-iut-limoges run build",
    "website:dev": "pnpm --filter signatures-iut-limoges-website run dev",
    "website:build": "pnpm --filter signatures-iut-limoges-website run build",
    "full:build": "pnpm run library:build && pnpm run website:build"
  },
  "private": true,
  "license": "MIT",
  "author": "Mikkel RINGAUD <mikkel@milescode.dev> (https://github.com/Vexcited)",
  "repository": "github:Vexcited/Signatures-IUT-Limoges"
}