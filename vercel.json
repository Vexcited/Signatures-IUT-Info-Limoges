{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  
  "installCommand": "pnpm install",
  "devCommand": "pnpm -w run website:dev",
  "buildCommand": "pnpm -w run full:build",

  "headers": [
    {
      "source": "/",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html"
        }
      ]
    }
  ]
}